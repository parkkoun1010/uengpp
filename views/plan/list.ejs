<!DOCTYPE html>
<html lang="en">
<%- include("../header");%>
<body>
<%- include("../menu");%>
<button type="button" class="btn btn-primary" onclick="location.href='/api/plan/new'">등록</button>
<div class="row row-cols-1 row-cols-md-3">
<% result.forEach(plan => { %>
  <div class="col mb-4">
    <!-- Card -->
    <div class="card">

      <!--Card content-->
      <div class="card-body">

        <!--Title-->
        <h4 class="card-title"><%= plan.subject %></h4>
        <!--Text-->
        <p class="card-text"><%= plan.starttime %> ~ <%= plan.finishtime %> / <%= plan.requiredtime %></p>
        <!-- Provides extra visual weight and identifies the primary action in a set of buttons -->
        <button type="button" class="btn btn-light-blue btn-md" onclick="location.href='/api/plan/<%=plan._id%>'">수정</button>

      </div>
    </div>
    <!-- Card -->
  </div>
  <%})%>
</div>
<%- include("../footer");%>
</body>
</html>
